<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Makcha" parent="Theme.AppCompat.Light">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/tossblue</item>
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="colorOnPrimary">@color/tossblue</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/tossblue</item>
        <item name="colorSecondaryVariant">@color/tossblue</item>
        <item name="colorOnSecondary">@color/tossblue</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="ActionBarTextColor" parent="android:TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#999999</item>
    </style>
</resources>